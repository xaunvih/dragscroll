        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-scss": "^2.6.1",
        "rollup-plugin-typescript": "^1.0.1",
        "sass": "^1.26.9",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "ts-loader": "^8.0.7",
        "tsc": "^1.20150623.0",
        "typescript": "^4.0.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2"
