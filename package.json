        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-typescript": "^7.12.1",
        "@types/autoprefixer": "^9.7.2",
        "autoprefixer": "^9.8.4",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^6.0.3",
        "css-loader": "^3.6.0",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^0.9.0",
        "npm-run-all": "^4.1.5",
        "path": "^0.12.7",
        "postcss": "^8.1.6",
